
q1. b)


data segment
a db 09h, 02h, 07h, 05h, 01h, 06h
len db 6
b db ?
data ends
code segment
assume cs:code, ds:data
start:
mov ax, data
mov ds, ax
lea si, a
mov bl, [si]
mov cl, len
call smallest
mov b, bl
mov ah, 4ch
int 21h


smallest proc near
go:
mov bh, [si]
cmp bh, bl
ja next
val:
mov bl, [si]
next:
inc si
loop go
ret
smallest endp 
